    "bootswatch": "^5.1.0",
    "cloudinary": "^1.27.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.1",
    "helmet": "^4.6.0",
    "jquery": "^3.6.0",
    "lottie": "0.0.1",
    "moment": "^2.29.1",
    "popper.js": "^1.16.1",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.2",
    "react-dom": "^17.0.2",
    "react-google-autocomplete": "^2.4.0",
    "react-loading": "^2.0.3",
    "react-lottie": "^1.2.3",
    "react-places-autocomplete": "^7.3.0",
    "react-redux": "^7.2.4",
    "react-responsive-carousel": "^3.2.22",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-toastify": "^7.0.4",
